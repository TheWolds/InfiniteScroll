{
    "presets": [
        [
            "@babel/env",
            {
                "useBuiltIns": "entry",
                "targets": {
                    "browsers": [
                        ">0.25%",
                        "not ie 11",
                        "not op_mini all"
                    ]
                }
            }
        ]
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/syntax-dynamic-import"
    ]
}